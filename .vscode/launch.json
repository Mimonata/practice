{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch powerset",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/a.out",
            "args": ["7", "1", "2", "3", "4", "-5"],  // Add arguments inside the brackets if needed
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build powerset"
        }
    ]
}
